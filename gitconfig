[include]
    path = ~/.gitconfig.local

[core]
    excludesfile = ~/.gitignore
    editor = /usr/bin/vim -i NONE -c \"set fenc=utf-8\"
    whitespace = trailing-space,space-before-tab

[color]
    ui = auto

[alias]
    br = branch
    ci = commit
    co = checkout
    df = diff
    lg = log -p
    glog = log --graph
    glo1 = log --graph --format=\"%C(yellow)%h%Creset %s\"
    st = status

[push]
    default = current
[credential]
    helper = osxkeychain
