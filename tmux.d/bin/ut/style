#!/bin/bash

PWD=$(cd $(dirname $0); pwd)
source ${PWD}/base
source ${TMUX_ROOT}/bin/utils

WINDOW_NAME=$(basename $0)
# echo ${WINDOW_NAME}

function main
{
    get-passwds

    tmux neww -n ${WINDOW_NAME} -c ${HOME}
    tmux send -t ${WINDOW_NAME}.0 "${TMUX_ROOT}/bin/auto_agent.sh ~/.ssh/id_rsa; ${HOME}/work/gh/ut/style" C-m
}

main "$@"
